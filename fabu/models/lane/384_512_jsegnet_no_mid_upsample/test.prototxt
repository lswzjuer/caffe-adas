    bias_filler {
      type: "constant"
      value: 0
    }
    dilation: 1
  }
}
layer {
  name: "ctx_final/bn"
  type: "BatchNorm"
  bottom: "ctx_final"
  top: "ctx_final"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
}
layer {
  name: "ctx_final/scale"
  type: "Scale"
  bottom: "ctx_final"
  top: "ctx_final"
